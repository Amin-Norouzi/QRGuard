<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.text.Font?>
<BorderPane prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="dev.aminnorouzi.qrguard.controller.HomeController">
    <center>

        <Label fx:id="label" BorderPane.alignment="CENTER">
            <font>
                <Font name="System Bold" size="48.0"/>
            </font>
        </Label>
    </center>
    <bottom>
        <Button onAction="#switchToLanding" text="Log out" BorderPane.alignment="CENTER"/>
    </bottom>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
    </padding>
</BorderPane>
